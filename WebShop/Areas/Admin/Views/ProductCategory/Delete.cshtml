@model ModelWeb.Models.ProductCategory

<h1>Are you sure you want to delete your product category?</h1>
<hr />

<form asp-action="Delete">
    <input type="hidden" name="id" value="@Model.Id"/>
    <input type="submit" value="Delete" class="btn btn-danger" />
</form>
<br />
<div>
    <a class="btn btn-primary" asp-action="Index" asp-route-productId="@Model.Id">Back to list</a>
</div>

@section Scripts{
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }

}